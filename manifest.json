  "manifest_version": 3,
  "action": {
    "default_popup": "TasksData.html",
    "default_icon": "icons/icon128.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["identity"],
  "oauth2": {
    "client_id": "1098520897598-agfpccghu0gfkienre2i68om3ermqj2j.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/tasks.readonly"]
  }
}
